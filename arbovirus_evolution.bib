keywords = {Aedes aegypti,C6/36,Dengue virus,Flavivirus,HuH-7,Mutation,NS4B,Toxorhynchites splendens,Vaccine,Vero},
number = {1},
pages = {222--232},
pmid = {12890635},
title = {{A trade-off in replication in mosquito versus mammalian systems conferred by a point mutation in the NS4B protein of dengue virus type 4}},
volume = {312},
year = {2003}
