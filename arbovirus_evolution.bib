file = {:C$\backslash$:/Users/Jennifer.Lord/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Fitzpatrick et al. - 2010 - Population variation of West Nile virus confers a host-specific fitness benefit in mosquitoes.pdf:pdf},
isbn = {0042-6822},
issn = {00426822},
journal = {Virology},
keywords = {Arbovirus,Fitness,Flavivirus,Mosquitoes,Quasispecies,West Nile virus},
