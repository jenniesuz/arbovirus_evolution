pmid = {28445723},
title = {{Mosquitoes Transmit Unique West Nile Virus Populations during Each Feeding Episode}},
volume = {19},
year = {2017}
}
@article{Grubaugh2016,
abstract = {The emergence of mosquito-borne RNA viruses, such as West Nile virus (WNV), is facilitated by genetically complex virus populations within hosts. Here, we determine whether WNV enzootic (Culex tarsalis, Cx. quinquefasciatus, and Cx. pipiens) and bridge vectors (Aedes aegypti) have differential impacts on viral mutational diversity and fitness. During systemic mosquito infection, WNV faced stochastic reductions in genetic diversity that rapidly was recovered during intra-tissue population expansions. Interestingly, this intrahost selection and diversification was mosquito species dependent with Cx. tarsalis and Cx. quinquefasciatus exhibiting greater WNV divergence. However, recovered viral populations contained a preponderance of potentially deleterious mutations (i.e., high mutational load) and had lower relative fitness in avian cells compared to input virus. These findings demonstrate that the adaptive potential associated with mosquito transmission varies depending on the mosquito species and carries a significant fitness cost in vertebrates.},
author = {Grubaugh, Nathan D. and Weger-Lucarelli, James and Murrieta, Reyes A. and Fauver, Joseph R. and Garcia-Luna, Selene M. and Prasad, Abhishek N. and Black, William C. and Ebel, Gregory D.},
