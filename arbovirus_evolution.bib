isbn = {1471-4922},
issn = {14715007},
journal = {Trends Parasitol.},
keywords = {Antiviral immunity,Arbovirus,Mosquito},
number = {3},
pages = {177--186},
pmid = {26626596},
title = {{Mosquito Defense Strategies against Viral Infection}},
volume = {32},
year = {2016}
}
